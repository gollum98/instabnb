{% extends 'base.html.twig' %}

{% block title %}{{ parent()}} - AnnonceController!{% endblock %}

{% block body %}
    {% include("add.html.twig") %}
    {{ "Exemple 1" }}</br></br>
    {% for value in value_array %}
        language : {{ value.language }}</br>
        version : {{ value.version }}</br></br>

    {% endfor %}</br></br>

    {{ "Exemple 2" }}</br></br>

    {% set tableau = ['1', 2, "dix-huit"] %}</br>
    {% for  i in range(0, 5, 1) %}
        {{ random(tableau) }}</br></br>
    {% endfor %}
    {{ date }}</br></br>
    {% set alph = range("a","z") %} {# créer un tableau allant de "a" à "z" #}
    {% for letter in alph %}
        {{ letter | upper }}{# force majuscule #}
    {% endfor %}
    {{ dump(alph) }} {# affiche alph#}
{% endblock %}